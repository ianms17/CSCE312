// Description:
// 		Proposition 5: Either the Apache software is installed or PHP is installed
//		p = apache is installed
//		q = php is installed
//		out = p xor q

CHIP Proposition5 {
	IN p, q;
	OUT out;
	
	PARTS:
	Xor(a=p, b=q, out=out);
}